
def colors():
    return [
        "black", 
        "brown", 
        "red", 
        "orange", 
        "yellow", 
        "green", 
        "blue", 
        "violet", 
        "grey", 
        "white",
    ]

def color_code(color):
        bonk = colors()
        return bonk.index(color)
    

print(color_code("red"))
